{
  "name": "@structurify/structurify",
  "version": "0.4.2",
  "description": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/structurify/structurify.git"
  },
  "author": "Marcin Mrotek",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/structurify/structurify/issues"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "build": "yarn db:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "setup": "yarn db:push && yarn db:generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:dev": "prisma migrate dev",
    "docker:linux": "docker compose up -d",
    "docker:windows": "docker compose up -d -f docker-compose.windows.yml"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@casl/ability": "^6.5.0",
    "@casl/prisma": "^1.4.1",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/terminus": "^10.1.1",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/exporter-jaeger": "^1.17.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.44.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.44.0",
    "@opentelemetry/instrumentation": "^0.44.0",
    "@opentelemetry/instrumentation-express": "^0.33.2",
    "@opentelemetry/instrumentation-http": "^0.44.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.33.2",
    "@opentelemetry/instrumentation-redis": "^0.35.2",
    "@opentelemetry/resources": "^1.17.1",
    "@opentelemetry/sdk-metrics": "^1.17.1",
    "@opentelemetry/sdk-node": "^0.44.0",
    "@opentelemetry/sdk-trace-base": "^1.17.1",
    "@opentelemetry/sdk-trace-node": "^1.17.1",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "@prisma/client": "5.5.2",
    "@prisma/extension-read-replicas": "^0.3.0",
    "@prisma/instrumentation": "^5.4.2",
    "@swc/core": "^1.3.96",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "2",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.10",
    "generate-password": "^1.7.0",
    "graphql": "^16.8.1",
    "graphql-query-complexity": "^0.12.0",
    "graphql-scalars": "^1.22.4",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.8",
    "meilisearch": "^0.35.0",
    "mjml": "^4.14.1",
    "module-alias": "^2.2.3",
    "nestjs-i18n": "^10.3.6",
    "nestjs-meilisearch": "^2.1.5",
    "nestjs-minio-client": "^2.1.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@faker-js/faker": "^8.2.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.2.8",
    "@swc/cli": "^0.1.62",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager-redis-store": "^2.0.3",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/minio": "^7.1.1",
    "@types/mjml": "^4.7.3",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^6.4.13",
    "@types/passport-jwt": "^3.0.12",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@yarnpkg/pnpify": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "helmet": "^7.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-createspyobj": "^2.0.0",
    "jest-ts-auto-mock": "^2.1.0",
    "k6": "^0.0.0",
    "prettier": "^3.0.0",
    "prisma": "5.5.2",
    "prisma-nestjs-graphql": "^19.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-auto-mock": "^3.7.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.89.0"
  },
  "resolutions": {
    "string-width": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "compiler": "ts-patch/compiler"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@app/(.*)$": "<rootDir>/../src/$1",
      "^@modules/(.*)$": "<rootDir>/../src/modules/$1",
      "^@shared/(.*)$": "<rootDir>/../src/shared/$1",
      "^@providers/(.*)$": "<rootDir>/../src/shared/providers/$1",
      "^@contracts/(.*)$": "<rootDir>/../src/contracts/$1",
      "^@decorators/(.*)$": "<rootDir>/../src/shared/decorators/$1"
    },
    "setupFiles": [
      "<rootDir>/../test/config.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed/init.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@1.22.19"
}
